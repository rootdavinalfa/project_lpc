<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lpc.control.mainControl">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File" />
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About LPC" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="produksi" layoutX="33.0" layoutY="167.0" mnemonicParsing="false" onAction="#btn_produksi" text="Produksi" />
            <Button fx:id="material" layoutX="264.0" layoutY="167.0" mnemonicParsing="false" onAction="#btn_warehouse" text="Warehouse Material" />
            <Button fx:id="mgmt" layoutX="487.0" layoutY="167.0" mnemonicParsing="false" text="Management" />
            <Button fx:id="cek_material" layoutX="124.0" layoutY="272.0" mnemonicParsing="false" onAction="#btn_cekmaterial" text="Cek Material" />
            <Label fx:id="lab_version" layoutX="19.0" layoutY="344.0" text="Version :" />
            <Button fx:id="fg" layoutX="386.0" layoutY="272.0" mnemonicParsing="false" onAction="#btn_fg" text="Finish Good" />
         </children>
    </AnchorPane>
  </children>
</VBox>
